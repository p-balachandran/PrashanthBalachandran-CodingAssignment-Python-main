      },
      {
        "type": "query",
        "name": "route",
        "label": "Route",
        "datasource": {
          "type": "prometheus",
          "uid": "prom"
        },
        "query": "label_values(http_requests_total, route)",
        "includeAll": true,
        "multi": true,
        "refresh": 1,
        "sort": 1,
        "current": {
          "text": "All",
          "value": [
            "$__all"
          ]
        }
      }
    ]
  },
  "panels": [
    {
      "type": "graph",
      "title": "End-to-End Latency p50 / p90 / p99 (s) \u2014 filtered by route",
      "id": 1,
      "datasource": {
        "type": "prometheus",
        "uid": "prom"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.5,  sum by (le, route) (rate(http_request_duration_seconds_bucket{route=~\"$route\"}[5m])))",
          "legendFormat": "p50 - {{route}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.9,  sum by (le, route) (rate(http_request_duration_seconds_bucket{route=~\"$route\"}[5m])))",
          "legendFormat": "p90 - {{route}}",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le, route) (rate(http_request_duration_seconds_bucket{route=~\"$route\"}[5m])))",
          "legendFormat": "p99 - {{route}}",
          "refId": "C"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "lines": true,
      "linewidth": 2
    },
    {
