      "title": "Requests per second \u2014 by route & status",
      "id": 2,
      "datasource": {
        "type": "prometheus",
        "uid": "prom"
      },
      "targets": [
        {
          "expr": "sum by (route, status) (rate(http_requests_total{route=~\"$route\"}[1m]))",
          "legendFormat": "{{route}} ({{status}})",
